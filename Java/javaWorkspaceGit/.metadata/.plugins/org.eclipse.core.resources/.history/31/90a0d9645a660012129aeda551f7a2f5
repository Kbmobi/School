import java.util.List;


public class Team {
	private String teamName;
	private List<HockeyPlayer> roster;
	
	public Team(String inTeamName) {
		teamName = inTeamName;
		roster = new ArrayList<HockeyPlayer>();
	}

	public void add(HockeyPlayer who) {
		roster.add(who);
	}
	public int size() { 
		int rosterSays = roster.size();
		return rosterSays;
	}
	
}
